{
  "version": 2,
  "name": "traders-helmet-academy",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "pages/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/index",
      "dest": "/index.html"
    },
    {
      "src": "/subscription",
      "dest": "/subscription.html"
    },
    {
      "src": "/subscription.html",
      "dest": "/subscription.html"
    },
    {
      "src": "/payment-processing",
      "dest": "/payment-processing.html"
    },
    {
      "src": "/payment-processing.html",
      "dest": "/payment-processing.html"
    },
    {
      "src": "/pages/auth/login",
      "dest": "/pages/auth/login.html"
    },
    {
      "src": "/pages/auth/login.html",
      "dest": "/pages/auth/login.html"
    },
    {
      "src": "/pages/auth/register",
      "dest": "/pages/auth/register.html"
    },
    {
      "src": "/pages/auth/register.html",
      "dest": "/pages/auth/register.html"
    },
    {
      "src": "/pages/auth/verify-email",
      "dest": "/pages/auth/verify-email.html"
    },
    {
      "src": "/pages/auth/verify-email.html",
      "dest": "/pages/auth/verify-email.html"
    },
    {
      "src": "/pages/dashboard",
      "dest": "/pages/dashboard/index.html"
    },
    {
      "src": "/pages/dashboard/",
      "dest": "/pages/dashboard/index.html"
    },
    {
      "src": "/pages/dashboard/index",
      "dest": "/pages/dashboard/index.html"
    },
    {
      "src": "/pages/dashboard/index.html",
      "dest": "/pages/dashboard/index.html"
    },
    {
      "src": "/pages/dashboard/gold",
      "dest": "/pages/dashboard/gold.html"
    },
    {
      "src": "/pages/dashboard/gold.html",
      "dest": "/pages/dashboard/gold.html"
    },
    {
      "src": "/pages/dashboard/platinum",
      "dest": "/pages/dashboard/platinum.html"
    },
    {
      "src": "/pages/dashboard/platinum.html",
      "dest": "/pages/dashboard/platinum.html"
    },
    {
      "src": "/pages/dashboard/diamond",
      "dest": "/pages/dashboard/diamond.html"
    },
    {
      "src": "/pages/dashboard/diamond.html",
      "dest": "/pages/dashboard/diamond.html"
    },
    {
      "src": "/pages/admin/login",
      "dest": "/pages/admin/login.html"
    },
    {
      "src": "/pages/admin/login.html",
      "dest": "/pages/admin/login.html"
    },
    {
      "src": "/pages/admin/dashboard",
      "dest": "/pages/admin/dashboard.html"
    },
    {
      "src": "/pages/admin/dashboard.html",
      "dest": "/pages/admin/dashboard.html"
    },
    {
      "src": "/pages/admin/user-management",
      "dest": "/pages/admin/user-management.html"
    },
    {
      "src": "/pages/admin/user-management.html",
      "dest": "/pages/admin/user-management.html"
    },
    {
      "src": "/.*",
      "dest": "/index.html",
      "status": 404
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
